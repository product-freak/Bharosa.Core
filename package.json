  "scripts": {
    "start": "prismixer && npm run prisma-generate && nx serve",
    "build": "nx build",
    "prisma-generate": "prisma generate --schema=\"./apps/Bharosa/src/app/common/DataStore/prisma/schema.prisma\"",
    "test": "nx test",
    "prisma_db_pull": "npx prisma db pull --schema=\"./apps/Bharosa/src/app/common/DataStore/prisma/schema.prisma\"",
    "prisma-deploy": "npx prisma migrate deploy --schema=\"./apps/Bharosa/src/app/common/DataStore/prisma/schema.prisma\"",
    "prisma-create": "npx prisma migrate dev --name update-post-salary-schema --schema=\"./apps/Bharosa/src/app/common/DataStore/prisma/schema.prisma\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build-dev"
    }
  },
  "lint-staged": {
    "*.{ts,scss,css,html,js,json,yml}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.ts": "eslint --cache --fix"
